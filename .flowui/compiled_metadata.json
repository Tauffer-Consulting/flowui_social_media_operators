    "YoutubeDownloadOperator": {
        "name": "YoutubeDownloadOperator",
        "dependency": {
            "docker_image": null,
            "dockerfile": "Dockerfile_youtube",
            "requirements_file": null
        },
        "tags": [],
        "style": {
            "node_label": "Youtube Download Operator",
            "node_type": "default",
            "node_style": {
                "backgroundColor": "#ebebeb"
            },
            "useIcon": true,
            "icon_class_name": "fa-brands fa-youtube",
            "iconStyle": {
                "cursor": "pointer"
            }
        },
